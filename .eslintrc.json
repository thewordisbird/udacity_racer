{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends":"eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-trailing-spaces": ["error", {"skipBlankLines": true}],
        "indent": "error",
        "semi": "error",
        "no-extra-semi": "error",
        "semi-spacing": "error"

    }
}